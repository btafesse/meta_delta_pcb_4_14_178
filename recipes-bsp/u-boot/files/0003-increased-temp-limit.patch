From 6ff1311e8b9d7da87d2e758c5171ff45c6f582ab Mon Sep 17 00:00:00 2001
From: btafesse <btafesse@mpr.com>
Date: Tue, 29 Jan 2019 15:56:36 -0500
Subject: [PATCH] increased temp limit

---
 arch/arm/dts/fsl-imx8mq.dtsi | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/dts/fsl-imx8mq.dtsi b/arch/arm/dts/fsl-imx8mq.dtsi
index 8e27a0e..deac59d 100644
--- a/arch/arm/dts/fsl-imx8mq.dtsi
+++ b/arch/arm/dts/fsl-imx8mq.dtsi
@@ -280,14 +280,14 @@
 			thermal-sensors = <&tmu>;
 			trips {
 				cpu_alert0: trip0 {
-					temperature = <85000>;
+					temperature = <125000>;
 					hysteresis = <2000>;
 					type = "passive";
 				};
 				cpu_crit0: trip1 {
-					temperature = <95000>;
+					temperature = <125000>;
 					hysteresis = <2000>;
-					type = "critical";
+					type = "passive";
 				};
 			};
 
